\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Note that
\begin_inset Formula 
\[
u_{t}=\varepsilon_{t}+\alpha\varepsilon_{t-1}+\alpha^{2}\varepsilon_{t-2}+\cdots
\]

\end_inset


\begin_inset Formula 
\[
\text{var}(u_{t})=\frac{\sigma^{2}}{1-\alpha^{2}}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\gamma(k) & =\text{cov}(u_{t},u_{t-k})\\
 & =\alpha^{k}\text{cov}(u_{t-k},u_{t-k})\\
 & =\alpha^{k}\frac{\sigma^{2}}{1-\alpha^{2}}
\end{align*}

\end_inset

Then
\begin_inset Formula 
\begin{align*}
\rho_{u}(k) & =\frac{\gamma(k)}{\text{var}(u_{t})}\\
 & =\alpha^{k}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Note that
\begin_inset Formula 
\begin{align*}
v_{t} & =u_{t}-u_{t-1}\\
 & =\alpha v_{t-1}+\varepsilon_{t}-\varepsilon_{t-1}\\
 & =\alpha^{2}v_{t-2}+\varepsilon_{t}-\varepsilon_{t-1}+\alpha(\varepsilon_{t-1}-\varepsilon_{t-2})\\
 & =\varepsilon_{t}+(\alpha-1)\varepsilon_{t-1}+\alpha(\alpha-1)\varepsilon_{t-2}+\cdots
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\text{var}(v_{t}) & =\sigma^{2}+(\alpha-1)^{2}\frac{\sigma^{2}}{1-\alpha^{2}}\\
 & =\frac{2(1-\alpha)}{1-\alpha^{2}}\sigma^{2}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\text{cov}(v_{t},v_{t-k}) & =\text{cov}(\alpha^{k}v_{t-k}-\varepsilon_{t-k},v_{t-k})\\
 & =\alpha^{k}\text{var}(v_{t-k})-\sigma^{2}
\end{align*}

\end_inset

Thena
\begin_inset Formula 
\begin{align*}
\rho_{v}(k) & =\alpha^{k}-\frac{1-\alpha^{2}}{2(1-\alpha)}\\
 & =\alpha^{k}-\frac{1}{2}(1+\alpha)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Note that
\begin_inset Formula 
\[
\rho_{u}(k)-\rho_{v}(k)=\frac{1}{2}(1+\alpha)
\]

\end_inset

which is increasing in 
\begin_inset Formula $\alpha$
\end_inset

.
\end_layout

\end_deeper
